{
  "task": {
    "taskDirectory": "./tasks",
    "filenameDelimiter": "-",
    "tasknameDelimiter": ":"
  },
  "plugin": {
    "debug": false,
    "pattern": [
      "gulp-*",
      "gulp.*"
    ],
    "scope": [
      "dependencies",
      "devDependencies",
      "peerDependencies"
    ],
    "camelize": true,
    "lazy": true
  }
}
